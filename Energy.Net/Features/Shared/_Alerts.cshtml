@if (TempData["WarningAlert"] != null)
{
    <div id="dangerAlert" class="alert alert-dismissible alert-danger">
        <button type="button" class="close" data-dismiss="alert">&times;</button>
        <strong>@TempData["WarningAlert"]</strong>
    </div>
}

@if (TempData["InfoAlert"] != null)
{
    <div id="infoAlert" class="alert alert-dismissible alert-info">
        <button type="button" class="close" data-dismiss="alert">&times;</button>
        <strong>@TempData["InfoAlert"]</strong>
    </div>
}
